{

  "name": "ATSocketMongoTypes",
  "description": "No description",
  "sparkStreamingWindow": 5000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "path": "myTestParquetPath"
  },
  "input": {
    "name": "in-socket",
    "type": "Socket",
    "configuration": {
      "hostname": "localhost",
      "port": "10666"
    }
  },
  "transformations": [{
    "name": "morphline-parser",
    "order": 0,
    "type": "Morphlines",
    "outputFields": [{
      "name": "product"
    },{
      "name": "price", "type":"double"
    },{
      "name": "text"
    }],
    "configuration": {
      "morphline": {
        "id": "morphline1",
        "importCommands": [
          "org.kitesdk.**"
        ],
        "commands": [{
          "readCSV": {
            "separator": ",",
            "columns": [
              "product",
              "price",
              "text"
            ],
            "trim": "true",
            "charset": "UTF-8",
            "quoteChar": "\"",
            "commentPrefix": "#"
          }
        }]
      }
    }
  }, {
    "name": "timestamp",
    "order": 1,
    "type": "DateTime",
    "outputFields": [{
      "name": "timestamp", "type": "long"
    }],
    "configuration": {
      "inputFormat": "autoGenerated"
    }
  }],
  "cubes": [{
    "name": "testCubeWithTime",
    "writer": {
      "outputs" : ["out-mongo"],
      "isAutoCalculatedId": true
    },
    "dimensions": [
      {
      "name": "product",
      "field": "product"
      },
      {
        "name": "price",
        "field": "price"
      },
      {
        "name": "minute",
        "field": "timestamp",
        "type": "DateTime",
        "precision": "minute",
        "computeLast": "10m"
      }
    ],
    "operators": [
      {
      "name": "count1",
      "type": "Count",
      "configuration": {}
      }
    ]
  }],
  "outputs": [{
    "name": "out-mongo",
    "type": "MongoDb",
    "configuration": {
      "hosts": [{
        "host": "localhost",
        "port": "60002"
      }],
      "dbName": "csvtesttypes"
    }
  }]
}