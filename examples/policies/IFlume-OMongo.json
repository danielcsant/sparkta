{
  "name": "policy-MongoDB-Financtial",
  "description": "policy-MongoDB-Financtial",
  "sparkStreamingWindow": 6000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "path": "myTestParquetPath"
  },
  "input":
    {
      "name": "in-flume",
      "type": "Flume",
      "configuration": {
        "type": "pull",
        "addresses": [{"host": "localhost", "port": "10999"}],
        "maxBatchSize": "500"
      }
    }
  ,
  "transformations": [
    {
      "name": "morphline-parser",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        {"name":"timestamp"},
        {"name": "extra1"}
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**",
            "com.stratio.ingestion.morphline.**"
          ],
          "commands": [
            {
              "readJson": {

              }
            },
            {
              "extractJsonPaths": {
                "paths": {
                 "timestamp": "/timestamp",
                  "extra1": "/extra1"

                }
              }
            },
            {
              "removeFields": {
                "blacklist": [
                  "literal:_attachment_body",
                  "literal:message"
                ]
              }
            }
          ]
        }
      }
    },
    {
      "name": "datetime",
      "order": 1,
      "inputField": "timestamp",
      "outputFields": [
        {"name":"timestamp"}
      ],
      "type": "DateTime",
      "configuration": {
        "timestamp": "unix"
      }
    }
  ],
  "cubes": [
    {
      "name": "extra1",
      "writer": {
        "outputs" : ["out-mongo"],
        "isAutoCalculatedId": true
      },
      "dimensions": [
        {
          "name": "extra1",
          "field": "extra1"
        },
        {
          "field": "timestamp",
          "precision": "minute",
          "type": "DateTime",
          "name": "minute",
          "computeLast": "10m"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "hosts": [{"host": "localhost" , "port": "27017" }],
        "dbName": "sparkta"
      }
    }
  ]
}