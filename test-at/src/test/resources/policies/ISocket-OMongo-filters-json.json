{

  "name": "ATSocketMongoFiltersJSON",
  "description": "No description",
  "sparkStreamingWindow": 5000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
  {
    "name": "in-socket",
    "type": "Socket",
    "configuration": {
      "hostname": "localhost",
      "port": "10666"
    }
  }
,
  "transformations": [
    {
      "name": "morphline",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        {"name":"hashtag"},
        {"name":"locationFirstLevel"},
        {"name":"locationSecondLevel"},
        {"name":"unique"}
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**","com.stratio.**"
          ],
          "commands": [
            {
              "readJson": {}
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "hashtag": "/hashtag",
                  "locationFirstLevel": "/locationFirstLevel",
                  "locationSecondLevel": "/locationSecondLevel",
                  "unique": "/unique"
                }
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "outputFields": [
        {"name":"time"}
      ],
      "configuration": {
        "inputFormat": "autoGenerated"
      }
    }
  ],
  "cubes": [
    {
      "name": "hashtagfilter",
      "writer": {
        "outputs" : ["out-mongo"],
        "isAutoCalculatedId": true
      },
      "dimensions": [
        {
          "name": "location",
          "field": "locationFirstLevel",
          "precision": "identity",
          "type": "Default"
        },
        {
          "name": "province",
          "field": "locationSecondLevel",
          "precision": "identity",
          "type": "Default"
        },
        {
          "name": "hashtag",
          "field": "hashtag",
          "precision": "identity",
          "type": "Default"
        },
        {
          "name": "minute",
          "field": "time",
          "type": "DateTime",
          "precision": "minute",
          "computeLast": "10m"
        }
      ],
      "operators": [
        {
          "name": "count1",
          "configuration": {
            "filters": [
              {
                "field": "hashtag",
                "type": "!=",
                "value": "",
                "fieldType": "string"
              },
              {
                "field": "unique",
                "type": "=",
                "value": "false",
                "fieldType": "string"
              }
            ]
          },
          "type": "Count"
        }
      ],
      "checkpointConfig": {
        "timeDimension": "time",
        "granularity": "minute",
        "interval": 100000,
        "timeAvailability": 90000
      }
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "hosts": [{"host": "localhost" , "port": "60001" }],
        "dbName": "csvtestfilter"
      }
    }
  ]
}