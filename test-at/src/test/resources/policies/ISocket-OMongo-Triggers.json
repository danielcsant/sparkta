{

  "name": "ATSocketMongoTriggers",
  "description": "No description",
  "sparkStreamingWindow": 5000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "path": "myTestParquetPath"
  },
  "input": {
    "name": "in-socket",
    "type": "Socket",
    "configuration": {
      "hostname": "localhost",
      "port": "10666"
    }
  },
  "transformations": [{
    "name": "morphline-parser",
    "order": 0,
    "type": "Morphlines",
    "outputFields": [{
      "name": "product"
    }, {
      "name": "price", "type": "double"
    }, {
      "name": "text"
    }],
    "configuration": {
      "morphline": {
        "id": "morphline1",
        "importCommands": [
          "org.kitesdk.**"
        ],
        "commands": [{
          "readCSV": {
            "separator": ",",
            "columns": [
              "product",
              "price",
              "text"
            ],
            "trim": "true",
            "charset": "UTF-8",
            "quoteChar": "\"",
            "commentPrefix": "#"
          }
        }]
      }
    }
  }, {
    "name": "timestamp",
    "order": 1,
    "type": "DateTime",
    "outputFields": [{
      "name": "timestamp",
      "type": "long"
    }],
    "configuration": {
      "inputFormat": "autoGenerated"
    }
  }],
  "streamTriggers": [
    {
      "name": "tableselect",
      "sql": "select * from stream",
      "outputs": ["out-mongo"]
    }
  ],
    "cubes": [
      {
        "name": "testCubeWithTime",
        "checkpointConfig": {
          "timeDimension": "timestamp",
          "granularity": "minute",
          "interval": 100000,
          "timeAvailability": 90000
        },
        "writer": {
          "isAutoCalculatedId": true
        },
        "dimensions": [
          {
            "name": "product",
            "field": "product"
          }
        ],
        "operators": [
          {
            "name": "count",
            "type": "Count",
            "configuration": {}
          }
        ],
        "triggers": [
          {
            "name": "tableproject",
            "sql": "select product from testCubeWithTime",
            "outputs": ["out-mongo"]
          },
          {
            "name": "tablewhere",
            "sql": "select product from testCubeWithTime where count > 8",
            "outputs": ["out-mongo"]
          }
        ]
      }
    ],
  "outputs": [{
    "name": "out-mongo",
    "type": "MongoDb",
    "configuration": {
      "hosts": [{
        "host": "localhost",
        "port": "60000"
      }],
      "dbName": "streamtest"
    }
  }]
}