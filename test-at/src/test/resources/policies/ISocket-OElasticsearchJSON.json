{

  "name": "ATSocketElasticsearch",
  "description": "No description",
  "sparkStreamingWindow": 5000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "path": "myTestParquetPath"
  },
  "input":
    {
      "name": "in-socket",
      "type": "Socket",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ,
  "transformations": [
    {
      "name": "morphline",
      "type": "Morphlines",
      "order": 0,
      "outputFields": [
        {"name":"tpe"},
        {"name":"smfProcess"},
        {"name":"millis"}
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readJson": {}
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "tpe": "/tpe",
                  "smfProcess": "/smfProcess/cod",
                  "millis": "/timeStamp/millis"
                }
              }
            },
            {
              "if": {
                "conditions": [
                  {
                    "not": {
                      "equals": {
                        "tpe": [
                          "A"
                        ]
                      }
                    }
                  }
                ],
                "then": [
                  {
                    "dropRecord": {}
                  }
                ]
              }
            }
          ]
        }
      }
    },
    {
      "name": "recorded_at_ms-parser",
      "order": 1,
      "type": "DateTime",
      "inputField": "millis",
      "outputFields": [
        {"name":"millis", "type": "long"}
      ],
      "configuration": {
        "inputFormat": "unixMillis"
      }
    }
  ],
  "cubes": [
    {
      "name": "cubeSmfProcess",
      "writer": {
        "outputs" : ["out-elasticsearch"],
        "dateType": "long",
        "isAutoCalculatedId": true
      },
      "dimensions": [
        {
          "field": "smfProcess",
          "name": "smfProcess"
        },
        {
          "name": "minute",
          "field": "millis",
          "type": "DateTime",
          "precision": "minute",
          "computeLast": "10m"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        }
      ]
    },
    {
      "name": "cubeMillis",
      "checkpointConfig": {
        "timeDimension": "millis",
        "granularity": "minute",
        "interval": 100000,
        "timeAvailability": 90000
      },
      "dimensions": [
        {
          "field": "millis",
          "name": "minute",
          "type": "DateTime",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-elasticsearch",
      "type": "ElasticSearch",
      "configuration": {
        "nodes": [{"node":"localhost","tcpPort":"9300","httpPort":"9200"}],
        "clusterName": "sparkta-elasticsearch",
        "indexMapping": "day"
      }
    }
  ]
}